<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{{title}}</title>
    {{#if styles}}
        {{{styles}}}
    {{/if}}
    {{#if style}}
        {{#each style}}
            <link rel="stylesheet" href="{{../URL}}{{href}}">
        {{/each}}
    {{/if}}
</head>
<body>
    {{{body}}}
    
    {{#if script}}
        {{#each script}}
            <script src="{{URL}}{{src}}"></script>
        {{/each}}
    {{/if}}
</body>
</html>